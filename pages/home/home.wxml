<view>
  <view class="table">
    <view class="tr bg-w">
      <view class="th">
        <checkbox-group bindchange="checkboxChange">
          <label class="checkbox">
            <checkbox value="true" checked="{{boxCheckedTotal}}"/>
          </label>
        </checkbox-group>
      </view>
      <view class="th">标题</view>
      <view class="th ">head3</view>
    </view>
    <!-- 表格 -->
     <block>
      <view class="tr bg-g" wx:for="{{DataList}}" wx:key="index">

        <view class="td">
          <checkbox-group bindchange="checkboxChangeChild" data-index="{{index}}">
            <label class="checkbox">
              <checkbox value="{{index}}" checked="{{item.phone}}"/>
            </label>
          </checkbox-group>
        </view>


        <view class="td">{{item.nickName}}</view>
        <view class="td ">{{index}}</view>
      </view>
     </block>
  </view>
  <button bindtap="btn">123</button>
</view>